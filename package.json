{
  "name": "halo",
  "description": "UnitedStack United Platform.",
  "version": "0.4.0",
  "private": true,
  "contributors": [
    "majiajun <majiajun@unitedstack.com>",
    "Lee Yao <yaoli@unitedstack.com>"
  ],
  "license": "MIT",
  "repository": "https://github.com/unitedstack/halo",
  "keywords": [
    "OpenStack",
    "API",
    "CMP"
  ],
  "main": "index.js",
  "scripts": {
    "build": "./scripts/halo_build.sh",
    "client_build": "./scripts/client_build.sh",
    "start": "./scripts/start.sh",
    "restart": "pm2 restart pm2.json",
    "test": "./scripts/test.sh",
    "eslint": "eslint *.js server/**/ configs/ scripts/ client/**/ tests/__tests__/ --ext .js,.jsx --cache && cd client && grunt jsonlint",
    "dev": "cd client && rm -rf dist/*.js dist/*.css dist/*.map && node ../scripts/client_i18n_build.js && webpack --config dev.webpack.config.js",
    "benchmark": "./scripts/benchmark.sh",
    "prepublish": "cd client/uskin && npm install",
    "transpile": "node ./scripts/client_i18n_build.js && node ./scripts/server_i18n_build.js",
    "assemble": "node ./scripts/assemble.js",
    "pull": "node ./scripts/pull.js && node ./scripts/merge_config.js",
    "merge": "node ./scripts/merge_config.js OVERWRITE",
    "release": "node ./scripts/get_tag.js",
    "tags": "node ./scripts/release.js",
    "add_eslint": "node ./scripts/add_eslint.js",
    "db_sync": "node ./scripts/cp_migrations.js && sequelize db:migrate"
  },
  "dependencies": {
    "async": "^1.5.0",
    "aws-sdk": "2.3.15",
    "babel-polyfill": "^6.26.0",
    "body-parser": "~1.14.1",
    "bufferutil": "^1.2.1",
    "ccap": "^0.6.10",
    "clipboard-plus": "^1.0.0",
    "co": "^4.6.0",
    "connect-memjs": "^0.2.1",
    "connect-redis": "^3.0.1",
    "cookie-parser": "^1.4.0",
    "echarts": "^3.3.2",
    "ejs": "^2.3.4",
    "errorhandler": "^1.4.2",
    "eventemitter2": "^1.0.0",
    "express": "~4.13.3",
    "express-session": "^1.12.1",
    "glob": "^6.0.1",
    "i18n-2": "^0.6.0",
    "json2csv": "^3.7.0",
    "lodash": "^4.17.0",
    "md5": "^2.1.0",
    "memjs": "^0.10.0",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "mysql": "^2.10.2",
    "node-uuid": "^1.4.7",
    "nodemailer": "^4.3.0",
    "paypal-rest-sdk": "1.6.9",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "redis": "^2.4.2",
    "sequelize": "^3.23.2",
    "superagent": "^3.5.0",
    "urlencode": "^1.1.0",
    "utf-8-validate": "^1.2.1",
    "vsphere-connect": "^1.1.0",
    "winston": "^2.1.1",
    "ws": "^1.0.1",
    "xml2json": "^0.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.1.12",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^17.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.1.4",
    "babel-runtime": "^6.3.19",
    "chalk": "^1.1.1",
    "clone": "^1.0.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.4.0",
    "deep-equal": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-cssnano-plus": "^1.0.0",
    "grunt-jsonlint": "^1.1.0",
    "grunt-webpack": "^2.0.1",
    "istanbul": "^0.4.1",
    "jasmine": "^2.4.1",
    "jest-cli": "^11.0.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.3",
    "load-grunt-tasks": "^3.3.0",
    "postcss-loader": "^1.3.2",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "^15.4.0",
    "rsvp": "^3.1.0",
    "supertest": "^1.1.0",
    "time-grunt": "^1.2.2",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.1"
  },
  "engines": {
    "node": ">= 5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest/index.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testDirectoryName": "tests/static_tests",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/client"
  }
}
